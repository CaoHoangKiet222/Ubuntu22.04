{
  "window.commandCenter": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "git.autofetch": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "vim.leader": "<space>",
  "vim.showMarksInGutter": false,
  "vim.foldfix": true,
  "vim.surround": true,
  "vim.easymotion": true,
  "vim.easymotionKeys": "asdghklqwertyuiopzxcvbnmfj",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.saveAll"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": ["workbench.action.closeWindow"]
    },
    {
      "before": ["<leader>", "\\"],
      "commands": ["workbench.action.splitEditorRight"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": ["workbench.action.splitEditorDown"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", ";"],
      "commands": ["workbench.action.closeEditorsInOtherGroups"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["workbench.action.showAllSymbols"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.files.action.showActiveFileInExplorer"]
    },
    {
      "before": ["<leader>", "g", "g"],
      "commands": ["workbench.view.scm"]
    },
    {
      "before": ["<leader>", "g", "c"],
      "commands": ["git.openChange"]
    },
    {
      "before": ["<leader>", "g", "p"],
      "commands": ["git.sync"]
    },
    {
      "before": ["<leader>", "s", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "s", "t"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["<leader>", "s", "r"],
      "commands": ["workbench.action.openRecent"]
    },
    {
      "before": ["<leader>", "s", "c"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "r", "n"],
      "commands": ["workbench.action.debug.start"]
    },
    {
      "before": ["<leader>", "r", "r"],
      "commands": ["workbench.action.debug.restart"]
    },
    {
      "before": ["<leader>", "r", "s"],
      "commands": ["workbench.action.debug.stop"]
    },
    {
      "before": ["<leader>", "d", "t"],
      "commands": ["editor.debug.action.toggleBreakpoint"]
    },
    {
      "before": ["<leader>", "d", "c"],
      "commands": ["workbench.action.debug.continue"]
    },
    {
      "before": ["<leader>", "d", "o"],
      "commands": ["workbench.action.debug.stepOver"]
    },
    {
      "before": ["<leader>", "d", "i"],
      "commands": ["workbench.action.debug.stepInto"]
    },
    {
      "before": ["<leader>", "d", "r"],
      "commands": ["editor.debug.action.showDebugHover"]
    },
    {
      "before": ["<leader>", "l", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "l", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "l", "c"],
      "commands": ["editor.action.changeAll"]
    },
    {
      "before": ["<leader>", "l", "m"],
      "commands": ["editor.action.refactor"]
    },
    {
      "before": ["<leader>", "t", "t"],
      "commands": ["workbench.action.terminal.toggleTerminal"]
    },
    {
      "before": ["<leader>", "s", "a"],
      "commands": ["workbench.action.showCommands"]
    }
  ],
  "vim.normalModeKeyBindings": [
    { "before": ["<C-h>"], "after": ["<C-w>", "h"] },
    { "before": ["<C-j>"], "after": ["<C-w>", "j"] },
    { "before": ["<C-k>"], "after": ["<C-w>", "k"] },
    { "before": ["<C-l>"], "after": ["<C-w>", "l"] },
    {
      "before": ["<A-n>"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<A-p>"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["editor.action.removeHighlight"]
    }
  ],
  "vim.visualModeKeyBindings": [
    { "before": ["<"], "after": ["<", "g", "v"] },
    { "before": [">"], "after": [">", "g", "v"] }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    },
    {
      "before": ["k", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.useSystemClipboard": true,
  "workbench.iconTheme": "material-icon-theme"
}
